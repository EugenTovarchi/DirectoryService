<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear />
        <add key="github" value="https://nuget.pkg.github.com/EugenTovarchi/index.json" />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    </packageSources>
    
    <packageSourceCredentials>
        <github>
            <add key="Username" value="EugenTovarchi" />
        </github>
    </packageSourceCredentials>
    
    <packageSourceMapping>
        <packageSource key="github">
            <package pattern="IstredDev.*" />
        </packageSource>
        
        <packageSource key="nuget.org">
            <package pattern="*" />
        </packageSource>
    </packageSourceMapping>
</configuration>